classDiagram
    class HealthcareService {
        +string identifier
        +bool active
        +string name
        +string comment
        +Reference providedBy
        +Reference location
        +Endpoint endpoint
    }

    class Organization {
        +string identifier
        +string name
        +Endpoint endpoint
    }

    class Location {
        +Address address
        +Telecom[] telecom
    }

    class Patient {
        +string identifier
        +Contact contact
    }

    class Contact {
        +Reference organization
        +Period period
    }

    class Period {
        +date start
        +date end
    }

    HealthcareService --> Organization : providedBy
    HealthcareService --> Location : location
    Patient --> Contact : contact
    Contact --> Period : period
